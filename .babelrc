{
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "dynamic-import-node",
      {
        "noInterop": true
      }
    ]
  ],
  "sourceMaps": true,
  "retainLines": true
}